#include "winver.h"

IDI_ICON1               ICON    DISCARDABLE     "power-supply.ico"

VS_VERSION_INFO VERSIONINFO
 FILEVERSION @PS-Management_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_MINOR@,0
 PRODUCTVERSION @PS-Management_VERSION_MAJOR@,@PROJECT_VERSION_MINOR@,@PROJECT_VERSION_MINOR@,0
 FILEFLAGS 0x0L
 FILEFLAGSMASK 0x3fL
 FILEOS 0x00040004L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "000004b0"
        BEGIN
            VALUE "CompanyName", "vitark"
            VALUE "FileDescription", "@PS-Management_DESCRIPTION@"
            VALUE "FileVersion", "@PS-Management_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_MINOR@.0"
            VALUE "LegalCopyright", "Copyright (c) 2021-@YEAR@ VitArk"
            VALUE "InternalName", "PS-Management"
            VALUE "OriginalFilename", "PS-Management.exe"
            VALUE "ProductName", "Power Supply Management"
            VALUE "ProductVersion", "@PS-Management_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_MINOR@.0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0, 1200
    END
END
